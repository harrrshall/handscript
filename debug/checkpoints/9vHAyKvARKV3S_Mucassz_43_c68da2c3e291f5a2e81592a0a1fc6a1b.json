          "type": "paragraph",
          "text": "Page No. 19"
        },
        {
          "type": "math",
          "latex": "\\tau_{max} = \\frac{Tc}{J}",
          "display": true
        },
        {
          "type": "math",
          "latex": "T = 14 \\times 10^3 \\text{ N/mm}^2",
          "display": true
        },
        {
          "type": "math",
          "latex": "c = 17^3",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\tau = \\frac{16T}{\\pi d^3} = \\frac{16 \\times 50 \\times 10^3}{\\pi(16)^3} = 62.169 \\text{ N/mm}^2",
          "display": true
        },
        {
          "type": "paragraph",
          "text": "Normal stress"
        },
        {
          "type": "math",
          "latex": "= 186.5 + 24.867 = 211.36 \\text{ N/mm}^2",
          "display": true
        },
        {
          "type": "paragraph",
          "text": "Now,"
        },
        {
          "type": "math",
          "latex": "\\sigma_1 = \\frac{\\sigma}{2} + \\frac{1}{2}\\sqrt{\\sigma^2+4\\tau^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= \\frac{211.36}{2} + \\frac{1}{2}\\sqrt{(211.36)^2+4(68.169)^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma_1 = 228.39 \\text{ N/mm}^2",
          "display": true
        },
        {
          "type": "heading",
          "level": 3,
          "text": "III"
        },
        {
          "type": "math",
          "latex": "\\sigma_{1,2} = \\frac{\\sigma}{2} \\pm \\frac{1}{2}\\sqrt{\\sigma^2+4\\tau^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma = 211.36",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma_2 = \\frac{211.36}{2} - \\frac{1}{2}\\sqrt{(211.36)^2+4(62 \\cdot 16.93)^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 105.68 - 122.618",
          "display": true
        },
        {
          "type": "math",
          "latex": "= -16.93 \\text{ N/mm}^2",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma_1 - \\sigma_2 = \\frac{\\sigma_y}{\\text{FOS}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "228.39 - (-16.93) = \\frac{400}{\\text{FOS}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "228.39 + 16.93 = \\frac{400}{\\text{FOS}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\text{FOS} = \\frac{400}{245.32}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\text{FOS} = 1.6305",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\text{FOS} = \\frac{\\sigma_y}{\\sigma_{\\text{Fos}}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\text{FOS} = \\frac{400}{228.83}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 1.748",
          "display": true
        }
      ]
    }
  ]
