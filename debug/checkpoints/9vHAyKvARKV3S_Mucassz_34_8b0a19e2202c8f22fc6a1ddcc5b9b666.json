        },
        {
          "type": "math",
          "latex": "= \\frac{10^4}{\\frac{3.14 (40^2 - 25^2)}{4}} = 13.065 \\frac{\\text{N}}{\\text{mm}^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\tau_{\\text{twist}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "T = \\frac{\\pi}{16} \\tau d_o^3 (1 - k^4)",
          "display": true
        },
        {
          "type": "math",
          "latex": "k = \\frac{d_i}{d_o}",
          "display": true
        },
        {
          "type": "math",
          "latex": "120 \\times 10^3 = \\frac{3.14 \\times \\tau \\times (40)^3}{16} \\left(1 - \\left(\\frac{25}{40}\\right)^4\\right)",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\tau = \\frac{120 \\times 10^3 \\times 16}{3.14 \\times (40)^3 \\left(1 - \\left(\\frac{25}{40}\\right)^4\\right)}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 11.274 \\frac{\\text{N}}{\\text{mm}^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma = \\text{Bending} + \\text{Axial} = 15.03 + 13.06 = 28.09 \\frac{\\text{N}}{\\text{mm}^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\tau_{\\text{max}} = \\frac{1}{2} \\sqrt{\\sigma^2 + 4\\tau^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= \\frac{1}{2} \\sqrt{(28.09)^2 + 4(11.27)^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 18.00 \\frac{\\text{N}}{\\text{mm}^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\sigma_{\\text{max}} = \\frac{1}{2} \\sigma + \\frac{1}{2} \\sqrt{\\sigma^2 + 4\\tau^2} = \\frac{28.09}{2} + 18.00",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 32.045 \\frac{\\text{N}}{\\text{mm}^2}",
