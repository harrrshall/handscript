        {
          "type": "math",
          "latex": "sigma_A = \\text{Axial} + \\sigma_{\\text{bending}}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 7.64 + 61.14 \\frac{N}{mm^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 68.78 \\frac{N}{mm^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "(\\sigma_B) = 61.14 - 7.64",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 53.5 \\frac{N}{mm^2}",
          "display": true
        },
        {
          "type": "heading",
          "level": 2,
          "text": "Twisting stress"
        },
        {
          "type": "math",
          "latex": "T = \\frac{\\pi}{16} \\tau d^3",
          "display": true
        },
        {
          "type": "math",
          "latex": "10^6 = \\frac{3.14}{16} \\times \\tau \\times (50)^3",
          "display": true
        },
        {
          "type": "math",
          "latex": "\\tau = \\frac{16 \\times 10^6}{3.14 \\times (50)^3} = 40.764 \\frac{N}{mm^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "(\\sigma_{\\text{max}})_A = \\frac{\\sigma}{2} + \\frac{1}{2} \\sqrt{\\sigma^2 + 4\\tau^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= \\frac{68.78}{2} + \\frac{1}{2} \\sqrt{(68.78)^2 + 4(40.76)^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= 34.39 + 53.32 = 87.71 \\frac{N}{mm^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "(\\sigma_{\\text{min}})_B = \\frac{\\sigma}{2} - \\frac{1}{2} \\sqrt{\\sigma^2 + 4\\tau^2}",
          "display": true
        },
        {
          "type": "math",
          "latex": "= \\frac{53.5}{2} + \\frac{1}{2} \\sqrt{(53.5)^2 + 4(40.76)^2}",
          "display": true
        }
      ]
    }
  ]
